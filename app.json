{
	"expo": {
		"name": "modelest-app",
		"slug": "modelest-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo2.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/logo2.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/logo2.png",
				"backgroundColor": "#000"
			}
		},
		"web": {
			"favicon": "./assets/logo2.png"
		}
	}
}
